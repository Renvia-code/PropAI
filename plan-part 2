---

## 8. Data Models

### 8.1 Convex Schema

```typescript
// convex/schema.ts

import { defineSchema, defineTable } from "convex/server";
import { v } from "convex/values";

export default defineSchema({
  // Organizations
  organizations: defineTable({
    name: v.string(),
    slug: v.string(),
    industry: v.string(),
    logo: v.optional(v.string()),
    website: v.optional(v.string()),
    description: v.optional(v.string()),
    timezone: v.string(),
    currency: v.string(),
    plan: v.string(),
    planLimits: v.object({
      chatsPerMonth: v.number(),
      voiceMinutesPerMonth: v.number(),
      storageBytes: v.number(),
      leadsPerMonth: v.number(),
      teamMembers: v.number(),
    }),
    usage: v.object({
      chatsThisMonth: v.number(),
      voiceMinutesThisMonth: v.number(),
      storageUsed: v.number(),
      leadsThisMonth: v.number(),
    }),
    settings: v.object({
      workingHours: v.any(),
      branding: v.any(),
      notifications: v.any(),
    }),
    createdAt: v.number(),
    updatedAt: v.number(),
  })
    .index("by_slug", ["slug"]),

  // Team Members
  users: defineTable({
    clerkId: v.string(),
    organizationId: v.id("organizations"),
    email: v.string(),
    name: v.string(),
    avatar: v.optional(v.string()),
    role: v.string(), // admin, manager, agent, viewer
    status: v.string(), // active, invited, deactivated
    workingHours: v.optional(v.any()),
    lastActiveAt: v.optional(v.number()),
    createdAt: v.number(),
  })
    .index("by_clerk_id", ["clerkId"])
    .index("by_organization", ["organizationId"]),

  // Contacts
  contacts: defineTable({
    organizationId: v.id("organizations"),
    name: v.string(),
    phone: v.optional(v.string()),
    email: v.optional(v.string()),
    avatar: v.optional(v.string()),
    location: v.optional(v.string()),
    tags: v.array(v.string()),
    metadata: v.any(),
    createdAt: v.number(),
    updatedAt: v.number(),
  })
    .index("by_organization", ["organizationId"])
    .index("by_phone", ["phone"]),

  // Conversations
  conversations: defineTable({
    organizationId: v.id("organizations"),
    contactId: v.id("contacts"),
    channel: v.string(), // whatsapp, facebook, instagram, line, web, sms, email
    channelConversationId: v.optional(v.string()),
    status: v.string(), // open, resolved, pending
    isAIHandling: v.boolean(),
    assigneeId: v.optional(v.id("users")),
    lastMessageAt: v.number(),
    lastMessage: v.object({
      text: v.string(),
      isFromContact: v.boolean(),
      isAI: v.boolean(),
    }),
    unreadCount: v.number(),
    metadata: v.any(),
    createdAt: v.number(),
    resolvedAt: v.optional(v.number()),
  })
    .index("by_organization", ["organizationId"])
    .index("by_organization_status", ["organizationId", "status"])
    .index("by_assignee", ["assigneeId"])
    .index("by_contact", ["contactId"]),

  // Messages
  messages: defineTable({
    conversationId: v.id("conversations"),
    senderType: v.string(), // contact, ai, agent
    senderId: v.optional(v.union(v.id("contacts"), v.id("users"))),
    content: v.string(),
    contentType: v.string(), // text, image, document, audio, video
    mediaUrl: v.optional(v.string()),
    status: v.string(), // sending, sent, delivered, read, failed
    metadata: v.any(),
    isInternal: v.boolean(),
    createdAt: v.number(),
  })
    .index("by_conversation", ["conversationId"]),

  // Leads
  leads: defineTable({
    organizationId: v.id("organizations"),
    contactId: v.id("contacts"),
    conversationId: v.optional(v.id("conversations")),
    status: v.string(), // new, contacted, qualified, site_visit, booked, negotiation, closed_won, closed_lost
    score: v.number(),
    scoreFactors: v.any(),
    source: v.string(), // whatsapp, facebook, voice, web, etc.
    assigneeId: v.optional(v.id("users")),
    requirements: v.object({
      propertyType: v.optional(v.string()),
      budget: v.optional(v.object({
        min: v.number(),
        max: v.number(),
      })),
      locations: v.array(v.string()),
      timeline: v.optional(v.string()),
      financing: v.optional(v.string()),
    }),
    tags: v.array(v.string()),
    notes: v.array(v.object({
      text: v.string(),
      authorId: v.id("users"),
      createdAt: v.number(),
    })),
    externalIds: v.any(), // CRM IDs
    createdAt: v.number(),
    updatedAt: v.number(),
    closedAt: v.optional(v.number()),
  })
    .index("by_organization", ["organizationId"])
    .index("by_organization_status", ["organizationId", "status"])
    .index("by_assignee", ["assigneeId"])
    .index("by_contact", ["contactId"]),

  // Properties/Projects
  properties: defineTable({
    organizationId: v.id("organizations"),
    name: v.string(),
    developer: v.optional(v.string()),
    rera: v.optional(v.string()),
    status: v.string(), // active, coming_soon, sold_out
    location: v.object({
      address: v.string(),
      area: v.string(),
      city: v.string(),
      pinCode: v.string(),
      coordinates: v.optional(v.object({
        lat: v.number(),
        lng: v.number(),
      })),
    }),
    possessionDate: v.optional(v.number()),
    images: v.array(v.string()),
    amenities: v.array(v.string()),
    unitTypes: v.array(v.object({
      type: v.string(),
      sizeRange: v.object({ min: v.number(), max: v.number() }),
      priceRange: v.object({ min: v.number(), max: v.number() }),
      totalUnits: v.number(),
      availableUnits: v.number(),
    })),
    isPublishedToAI: v.boolean(),
    metadata: v.any(),
    createdAt: v.number(),
    updatedAt: v.number(),
  })
    .index("by_organization", ["organizationId"])
    .index("by_organization_status", ["organizationId", "status"]),

  // Site Visits
  siteVisits: defineTable({
    organizationId: v.id("organizations"),
    leadId: v.id("leads"),
    propertyId: v.id("properties"),
    agentId: v.id("users"),
    scheduledAt: v.number(),
    status: v.string(), // scheduled, confirmed, completed, cancelled, no_show
    outcome: v.optional(v.string()),
    unitsShown: v.array(v.string()),
    feedback: v.optional(v.string()),
    nextAction: v.optional(v.object({
      type: v.string(),
      scheduledAt: v.number(),
    })),
    reminders: v.object({
      confirmationSent: v.boolean(),
      dayBeforeSent: v.boolean(),
      hoursBeforeSent: v.boolean(),
    }),
    createdAt: v.number(),
    completedAt: v.optional(v.number()),
  })
    .index("by_organization", ["organizationId"])
    .index("by_organization_date", ["organizationId", "scheduledAt"])
    .index("by_agent_date", ["agentId", "scheduledAt"])
    .index("by_lead", ["leadId"]),

  // Voice Calls
  voiceCalls: defineTable({
    organizationId: v.id("organizations"),
    phoneNumber: v.string(),
    contactId: v.optional(v.id("contacts")),
    leadId: v.optional(v.id("leads")),
    direction: v.string(), // inbound, outbound
    campaignId: v.optional(v.id("voiceCampaigns")),
    duration: v.number(),
    recordingUrl: v.optional(v.string()),
    transcript: v.optional(v.string()),
    outcome: v.string(),
    aiScore: v.optional(v.number()),
    aiAnalysis: v.optional(v.any()),
    transferredTo: v.optional(v.id("users")),
    createdAt: v.number(),
  })
    .index("by_organization", ["organizationId"])
    .index("by_campaign", ["campaignId"]),

  // Voice Campaigns
  voiceCampaigns: defineTable({
    organizationId: v.id("organizations"),
    name: v.string(),
    status: v.string(), // draft, scheduled, running, paused, completed
    script: v.string(),
    contacts: v.array(v.object({
      phone: v.string(),
      name: v.optional(v.string()),
      variables: v.any(),
    })),
    stats: v.object({
      totalContacts: v.number(),
      called: v.number(),
      answered: v.number(),
      leads: v.number(),
      siteVisits: v.number(),
    }),
    scheduledAt: v.optional(v.number()),
    completedAt: v.optional(v.number()),
    createdAt: v.number(),
  })
    .index("by_organization", ["organizationId"]),

  // Knowledge Base Files
  knowledgeFiles: defineTable({
    organizationId: v.id("organizations"),
    name: v.string(),
    type: v.string(), // pdf, docx, txt, csv, xlsx
    size: v.number(),
    storageKey: v.string(),
    status: v.string(), // uploading, processing, indexed, failed
    chunkCount: v.optional(v.number()),
    usageCount: v.number(),
    createdAt: v.number(),
    indexedAt: v.optional(v.number()),
  })
    .index("by_organization", ["organizationId"]),

  // Knowledge Chunks (for RAG)
  knowledgeChunks: defineTable({
    organizationId: v.id("organizations"),
    sourceType: v.string(), // file, database, crawler, qa
    sourceId: v.string(),
    content: v.string(),
    embedding: v.array(v.float64()),
    metadata: v.any(),
    createdAt: v.number(),
  })
    .index("by_organization", ["organizationId"])
    .vectorIndex("by_embedding", {
      vectorField: "embedding",
      dimensions: 1536,
      filterFields: ["organizationId"],
    }),

  // Channels
  channels: defineTable({
    organizationId: v.id("organizations"),
    type: v.string(), // whatsapp, facebook, instagram, line, sms, email, web
    status: v.string(), // connected, disconnected, pending
    config: v.any(),
    stats: v.object({
      conversationsThisMonth: v.number(),
      leadsThisMonth: v.number(),
    }),
    connectedAt: v.optional(v.number()),
    createdAt: v.number(),
  })
    .index("by_organization", ["organizationId"]),

  // Integrations
  integrations: defineTable({
    organizationId: v.id("organizations"),
    type: v.string(), // selldo, zoho, hubspot, salesforce, zapier, etc.
    status: v.string(), // connected, disconnected
    config: v.any(),
    lastSyncAt: v.optional(v.number()),
    createdAt: v.number(),
  })
    .index("by_organization", ["organizationId"]),

  // Webhooks
  webhooks: defineTable({
    organizationId: v.id("organizations"),
    name: v.string(),
    url: v.string(),
    events: v.array(v.string()),
    isActive: v.boolean(),
    secret: v.string(),
    successRate: v.number(),
    lastTriggeredAt: v.optional(v.number()),
    createdAt: v.number(),
  })
    .index("by_organization", ["organizationId"]),

  // Activity Logs
  activityLogs: defineTable({
    organizationId: v.id("organizations"),
    userId: v.optional(v.id("users")),
    action: v.string(),
    entityType: v.string(),
    entityId: v.string(),
    metadata: v.any(),
    createdAt: v.number(),
  })
    .index("by_organization", ["organizationId"]),
});
```

